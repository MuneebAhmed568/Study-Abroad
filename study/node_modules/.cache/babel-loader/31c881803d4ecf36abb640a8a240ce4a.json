pe':'application/json',\n        Authorization:`Bearer ${this.token}`\n      }})\n            .then((response) => {\n            this.leade = response.data\n\t\t    // ////console.log(this.ConsultantsData) \n            }) \n        }\n        \n    }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}