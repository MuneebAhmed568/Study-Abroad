{"ast":null,"code":"// import config from '@/../config'\n//import SliderImg from '@/SliderImages.json'\nimport Adds from \"@/components/CorousalSlide\";\nimport SideAdds from \"@/components/SideCorousalSlide\";\nimport Form from \"@/components/MainForm\";\nexport default {\n  props: [\"data\"],\n\n  data() {\n    return {\n      from_page: \"test\",\n      id: this.$route.params.name,\n      datas: \"\"\n    };\n  },\n\n  components: {\n    Adds,\n    SideAdds,\n    Form\n  },\n\n  async mounted() {\n    this.from_page = \"test\";\n    var testcard1 = document.getElementById(\"ielt\");\n    var testcard2 = document.getElementById(\"pte\");\n    var testcard3 = document.getElementById(\"toefl\");\n    var testcard4 = document.getElementById(\"sat\");\n    var testcard5 = document.getElementById(\"gre\");\n\n    if (this.id == \"IELTS\") {\n      testcard1.style.display = \"block\"; //or\n\n      testcard1.style.visibility = \"visible\";\n      testcard2.style.display = \"none\"; //or\n\n      testcard2.style.visibility = \"hidden\";\n      testcard3.style.display = \"none\"; //or\n\n      testcard3.style.visibility = \"hidden\";\n      testcard4.style.display = \"none\"; //or\n\n      testcard4.style.visibility = \"hidden\";\n      testcard5.style.display = \"none\"; //or\n\n      testcard5.style.visibility = \"hidden\";\n    } else if (this.id == \"PTE\") {\n      testcard1.style.display = \"none\"; //or\n\n      testcard1.style.visibility = \"hidden\";\n      testcard2.style.display = \"block\"; //or\n\n      testcard2.style.visibility = \"visible\";\n      testcard3.style.display = \"none\"; //or\n\n      testcard3.style.visibility = \"hidden\";\n      testcard4.style.display = \"none\"; //or\n\n      testcard4.style.visibility = \"hidden\";\n      testcard5.style.display = \"none\"; //or\n\n      testcard5.style.visibility = \"hidden\";\n    } else if (this.id == \"TOEFL\") {\n      testcard1.style.display = \"none\"; //or\n\n      testcard1.style.visibility = \"hidden\";\n      testcard2.style.display = \"none\"; //or\n\n      testcard2.style.visibility = \"hidden\";\n      testcard3.style.display = \"block\"; //or\n\n      testcard3.style.visibility = \"visible\";\n      testcard4.style.display = \"none\"; //or\n\n      testcard4.style.visibility = \"hidden\";\n      testcard5.style.display = \"none\"; //or\n\n      testcard5.style.visibility = \"hidden\";\n    } else if (this.id == \"SAT\") {\n      testcard1.style.display = \"none\"; //or\n\n      testcard1.style.visibility = \"hidden\";\n      testcard2.style.display = \"none\"; //or\n\n      testcard2.style.visibility = \"hidden\";\n      testcard3.style.display = \"none\"; //or\n\n      testcard3.style.visibility = \"hidden\";\n      testcard4.style.display = \"block\"; //or\n\n      testcard4.style.visibility = \"visible\";\n      testcard5.style.display = \"none\"; //or\n\n      testcard5.style.visibility = \"hidden\";\n    } else if (this.id == \"GRE\") {\n      testcard1.style.display = \"none\"; //or\n\n      testcard1.style.visibility = \"hidden\";\n      testcard2.style.display = \"none\"; //or\n\n      testcard2.style.visibility = \"hidden\";\n      testcard3.style.display = \"none\"; //or\n\n      testcard3.style.visibility = \"hidden\";\n      testcard4.style.display = \"none\"; //or\n\n      testcard4.style.visibility = \"hidden\";\n      testcard5.style.display = \"block\"; //or\n\n      testcard5.style.visibility = \"visible\";\n    } else {\n      //console.log(response,\"eeroor\")\n      this.$router.push({\n        name: \"notFound\"\n      });\n    } // await this.axios\n    //     .get(config.hostname + \"api/Testsinfo/GetbyName/\" + this.id)\n    //     .then((response) => {\n    //         if(response.data.length >0){\n    //       this.datas = response.data;\n    //       //console.log(this.datas.About, \"testing Test Single page \")\n    //         }\n    //       else{\n    //         console.log(response,\"eeroor\")\n    //         this.$router.push({name: 'notFound'});\n    //       }\n    //     });\n\n  }\n\n};","map":{"version":3,"mappings":"AAyyBA;AACA;AACA,OAAOA,IAAP,MAAiB,4BAAjB;AACA,OAAOC,QAAP,MAAqB,gCAArB;AACA,OAAOC,IAAP,MAAiB,uBAAjB;AAEA,eAAe;AACbC,OAAK,EAAE,CAAC,MAAD,CADM;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,MADN;AAELC,QAAE,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,IAFlB;AAGLC,WAAK,EAAE;AAHF,KAAP;AAKD,GARY;;AASbC,YAAU,EAAE;AACVX,QADU;AAEVC,YAFU;AAGVC;AAHU,GATC;;AAcb,QAAMU,OAAN,GAAgB;AACd,SAAKP,SAAL,GAAiB,MAAjB;AACA,QAAIQ,SAAQ,GAAIC,QAAQ,CAACC,cAAT,CAAwB,MAAxB,CAAhB;AACA,QAAIC,SAAQ,GAAIF,QAAQ,CAACC,cAAT,CAAwB,KAAxB,CAAhB;AACA,QAAIE,SAAQ,GAAIH,QAAQ,CAACC,cAAT,CAAwB,OAAxB,CAAhB;AACA,QAAIG,SAAQ,GAAIJ,QAAQ,CAACC,cAAT,CAAwB,KAAxB,CAAhB;AACA,QAAII,SAAQ,GAAIL,QAAQ,CAACC,cAAT,CAAwB,KAAxB,CAAhB;;AACA,QAAI,KAAKT,EAAL,IAAW,OAAf,EAAwB;AACtBO,eAAS,CAACO,KAAV,CAAgBC,OAAhB,GAA0B,OAA1B,CADsB,CACa;;AACnCR,eAAS,CAACO,KAAV,CAAgBE,UAAhB,GAA6B,SAA7B;AACAN,eAAS,CAACI,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAHsB,CAGY;;AAClCL,eAAS,CAACI,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAL,eAAS,CAACG,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CALsB,CAKY;;AAClCJ,eAAS,CAACG,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAJ,eAAS,CAACE,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAPsB,CAOY;;AAClCH,eAAS,CAACE,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAH,eAAS,CAACC,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CATsB,CASY;;AAClCF,eAAS,CAACC,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACF,KAXA,MAWO,IAAI,KAAKhB,EAAL,IAAW,KAAf,EAAsB;AAC3BO,eAAS,CAACO,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAD2B,CACO;;AAClCR,eAAS,CAACO,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAN,eAAS,CAACI,KAAV,CAAgBC,OAAhB,GAA0B,OAA1B,CAH2B,CAGQ;;AACnCL,eAAS,CAACI,KAAV,CAAgBE,UAAhB,GAA6B,SAA7B;AACAL,eAAS,CAACG,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAL2B,CAKO;;AAClCJ,eAAS,CAACG,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAJ,eAAS,CAACE,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAP2B,CAOO;;AAClCH,eAAS,CAACE,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACAH,eAAS,CAACC,KAAV,CAAgBC,OAAhB,GAA0B,MAA1B,CAT2B,CASO;;AAClCF,eAAS,CAACC,KAAV,CAAgBE,UAAhB,GAA6B,QAA7B;AACF,KAXO,MAWA,IAAI,KAAKhB,EAAL,IAAW,OAAf,EAAwB;AAC7BO,eAAS,CAACO,KAAV,CAAgB