{"ast":null,"code":"import config from '@/../config';\nimport { create } from 'ipfs-http-client';\nimport { Buffer } from 'buffer';\nexport default {\n  props: ['toggle', 'emailsession', 'emails'],\n\n  data() {\n    return {\n      file: \"\",\n      image: null,\n      projectId: '2DBOP6lMpJJXMfPYTHHbUprAtAA',\n      projectSecret: 'c47db02a18967d4b6b1758b83a53ffb8',\n      token: \"\"\n    };\n  },\n\n  mounted() {\n    this.token = localStorage.getItem(\"token\");\n  },\n\n  methods: {\n    async uploadToIpfs(file) {\n      const auth = 'Basic ' + Buffer.from(this.projectId + ':' + this.projectSecret).toString('base64');\n      const client = await create({\n        host: 'ipfs.infura.io',\n        port: 5001,\n        protocol: 'https',\n        headers: {\n          authorization: auth\n        }\n      });\n      const added = await client.add(file);\n      const url = `https://abroadstudy.infura-ipfs.io/ipfs/${added.path}`;\n      return url;\n    },\n\n    async updateinfo() {\n      var url = await this.uploadToIpfs(this.file); //console.log(\"Url from Ads Uni\");\n      //console.log(url);\n\n      var select = document.getElementById(\"location\");\n      var selectedValue = select.options[select.selectedIndex].value;\n      this.email = document.getElementById('studentemail').value;\n      this.phoneno = document.getElementById('phonenumber').value;\n      this.name = document.getElementById('studentname').value;\n\n      if (this.phoneno != \"\" && this.name != \"\" && this.file != \"\") {\n        ////console.log(this.compname + \" \"+this.email +\"\" + this.pername + \" \" + this.city + \" \" +  this.address + \" \" + this.phno + \" \" + this.web + \" \"  + this.freetxt+ \" \")\n        await this.axios.put(config.hostname + 'api/Studentinfo/Putbyemail/', {\n          Fullname: this.name,\n          Phoneno: this.phoneno,\n          Email: this.email,\n          Location: selectedValue\n        }, {\n          headers: {\n            Accept: 'application/json',\n            'Content-type': 'application/json',\n            Authorization: `Bearer ${this.token}`\n          }\n        }).then(response => {\n          alert(response.data);\n        }); ////console.log(this.file)\n\n        if (this.file != \"\") {\n          const fd = new FormData();\n          this.email = document.getElementById('studentemail').value;\n          fd.append(\"Email\", this.email);\n          fd.append('imageurlbase64', url); ////console.log(fd)\n\n          await this.axios.post(config.hostname + 'api/Studentinfo/savepic/', fd, {\n            headers: {\n              Accept: 'application/json',\n              'Content-type': 'application/json',\n              Authorization: `Bearer ${this.token}`\n            }\n          }).then(res => {\n            res; //alert(res.data)\n          });\n        }\n      } else {\n        alert(\"All Fields are Required\");\n      }\n    },\n\n    onFilePicked(event) {\n      this.file = event.target.files[0];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2RA,OAAOA,MAAP,MAAmB,aAAnB;AAEA,SAASC,MAAT,QAAuB,kBAAvB;AACA,SAASC,MAAT,QAAuB,QAAvB;AAEA,eAAe;AACdC,OAAK,EAAE,CAAC,QAAD,EAAU,cAAV,EAAyB,QAAzB,CADO;;AAEdC,MAAI,GACJ;AACC,WAAM;AACLC,UAAI,EAAC,EADA;AAELC,WAAK,EAAE,IAFF;AAIIC,eAAS,EAAE,6BAJf;AAKIC,mBAAY,EAAI,kCALpB;AAMLC,WAAK,EAAC;AAND,KAAN;AAQD,GAZc;;AAedC,SAAM,GAAK;AACV,SAAKD,KAAL,GAAYE,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAZ;AAEA,GAlBa;;AAmBdC,SAAO,EACP;AACC,UAAMC,YAAN,CAAmBT,IAAnB,EAAwB;AAElB,YAAMU,IAAG,GAAG,WAAWb,MAAM,CAACc,IAAP,CAAY,KAAKT,SAAL,GAAiB,GAAjB,GAAuB,KAAKC,aAAxC,EAAuDS,QAAvD,CAAgE,QAAhE,CAAvB;AAEA,YAAMC,MAAK,GAAI,MAAMjB,MAAM,CAAC;AACxBkB,YAAI,EAAE,gBADkB;AAExBC,YAAI,EAAE,IAFkB;AAGxBC,gBAAQ,EAAE,OAHc;AAIxBC,eAAO,EAAE;AACTC,uBAAa,EAAER;AADN;AAJe,OAAD,CAA3B;AAQA,YAAMS,KAAI,GAAI,MAAMN,MAAM,CAACO,GAAP,CAAWpB,IAAX,CAApB;AACA,YAAMqB,GAAE,GAAK,2CAA0CF,KAAK,CAACG,IAAK,EAAlE;AACA,aAAOD,GAAP;AACH,KAhBJ;;AAiBC,UAAME,UAAN,GACA;AACC,UAAIF,GAAG,GAAE,MAAM,KAAKZ,YAAL,CAAkB,KAAKT,IAAvB,CAAf,CADD,CAEU